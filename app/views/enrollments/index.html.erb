<%= link_to 'Users' , users_path %>
<%= link_to 'Subjects', subjects_path %>
<%= link_to 'Courses', courses_path %>
<%= link_to 'Instructors', instructors_path %>
<%= link_to 'Search', enrollments_path %>
<%= link_to 'Log Out', sessions_destroy_url %>


<h1>Search for a course</h1>
<p>You can search by course, subject, or both</p>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @enrollments.each do |enrollment| %>
      <tr>
        <td><%= link_to 'Show', enrollment %></td>
        <td><%= link_to 'Edit', edit_enrollment_path(enrollment) %></td>
        <td><%= link_to 'Destroy', enrollment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Enrollment', new_enrollment_path %>
